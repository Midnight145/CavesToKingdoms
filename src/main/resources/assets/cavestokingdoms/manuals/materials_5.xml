         <armor>minecraft:chainmail_helmet</armor>
         <armor>minecraft:chainmail_chestplate</armor>
         <armor>minecraft:chainmail_boots</armor>
         <armor>minecraft:chainmail_leggings</armor>
      </armors>
      <title>Iron</title>
      <requires>discover.cavestokingdoms.iron</requires>
      <requiresIcon>minecraft:iron_ingot</requiresIcon>
